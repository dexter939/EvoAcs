modules = ["php-8.3"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["php83", "php83Packages.composer", "php83Extensions.pdo_mysql", "php83Extensions.soap", "php83Extensions.redis"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "ACS Server"

[[workflows.workflow]]
name = "ACS Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "php artisan serve --host=0.0.0.0 --port=5000"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 34665
externalPort = 3000

[[ports]]
localPort = 45419
externalPort = 3001
