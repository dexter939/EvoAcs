modules = ["php-8.3", "postgresql-16"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["php83", "php83Packages.composer", "php83Extensions.pdo_mysql", "php83Extensions.soap", "php83Extensions.redis", "protobuf"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "ACS Server"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Queue Worker"

[[workflows.workflow]]
name = "ACS Server"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pkill -9 php 2>/dev/null || true; sleep 1; php artisan serve --host=0.0.0.0 --port=5000"
waitForPort = 5000

[[workflows.workflow]]
name = "Queue Worker"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "php artisan queue:work --sleep=3 --tries=3 --timeout=120"

[workflows.workflow.metadata]
outputType = "console"

